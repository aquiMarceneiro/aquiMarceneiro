/* HOME HERO */
#hero { height:480px; position:relative; padding:0px; background: white;}

#hero #slider { float:left; width:100%; height:480px; position:relative; opacity:0.6; }
#hero #slider .slide { position:absolute; top:0px; left:0px; width:100%; height:100%; background-position:center; background-size:cover; z-index:0; opacity:0; transition:opacity 0s; }
#hero #slider .slide .filter { position:absolute; top:0px; left:0px; width:100%; height:100%; background:-moz-linear-gradient(top, rgba(255,255,255,0.3) 0%, rgba(255,255,255,0.3) 77%, rgba(255,255,255,1) 100%); background:-webkit-linear-gradient(top, rgba(255,255,255,0.3) 0%,rgba(255,255,255,0.3) 77%,rgba(255,255,255,1) 100%); background:linear-gradient(to bottom, rgba(255,255,255,0.3) 0%,rgba(255,255,255,0.3) 77%,rgba(255,255,255,1) 100%); filter:progid:DXImageTransform.Microsoft.gradient( startColorstr='#4dffffff', endColorstr='#ffffff',GradientType=0 );}
#hero #slider .slide.last { z-index:1; opacity:1; }
#hero #slider .slide.active { z-index:2; opacity:1; transition:opacity 1s; }

#hero .content { position:absolute; top:0px; left:0px; width:1170px; height:100%; left:50%; margin-left:-585px; z-index:3; }
#hero .content h2 { position:absolute; top:160px; left:0px; width:740px; font-size:60px; color:white; text-shadow:0px 2px 5px rgba(0, 0, 0, 0.5); }

#hero .content .textoBanner{ float:left; position: absolut; margin-top: 60px;  font-size: 60px; color: white; }

#hero .content .search-box { position:absolute; width:370px; height:380px; top:80px; right:0px; }
#hero .content .search-box h3 { position:absolute; top:-60px; left:60px; font-size:24px; text-align:center; background-color:#FFF; padding:15px 20px; border-radius:5px; }
#hero .content .search-box h3 strong { color:white; }

#hero .content .search-box .buttons { position:absolute; top:0px; left:0px; width:100%; height:100%; padding-top:50px; z-index:3; }
#hero .content .search-box .buttons .btn { float:left; width:100%; margin-bottom:20px; padding:30px 20px; font-size:30px; box-sizing:border-box !important; }

#hero .content .search-box .search-area { opacity:0; position:absolute; top:0px; left:0px; width:100%; height:100%; z-index:2; }
#hero .content .search-box .search-area.active { opacity:1; }

#hero .content .search-box .search-area .header { float:left; width:100%; }
#hero .content .search-box .search-area .header span { float:left; width:50%; padding:10px 0px; font-size:18px; font-weight:700; text-align:center; opacity:0.7; cursor:pointer; border-top-right-radius:6px; border-top-left-radius:6px; }
#hero .content .search-box .search-area .header span.active,
#hero .content .search-box .search-area .header span:hover { opacity:1; }
#hero .content .search-box .search-area .header span:first-child { background-color:white; color:#532F08; }
#hero .content .search-box .search-area .header span:last-child { background-color:#532F08; color:white; }

#hero .content .search-box .search-area .search { float:left; }
#hero .content .search-box .search-area .search.profissional { background-color:white; }
#hero .content .search-box .search-area .search.orcamento { background-color:#532F08; }

#hero .content .search-box .search-area form { float:left; width:100%; padding:20px; border-bottom-right-radius:6px; border-bottom-left-radius:6px; box-sizing:border-box; }
#hero .content .search-box .search-area form select { float:left; width:100%; margin-bottom:10px; }
#hero .content .search-box .search-area form .form-input { float:left; position:relative; width:100%; margin-bottom:10px; }
#hero .content .search-box .search-area form .form-input input { width:100%; padding-right:35px; }
#hero .content .search-box .search-area form .form-input img { display:none; position:absolute; top:15px; right:15px; cursor:pointer; transform:rotate(0deg); transition:all 0.15s; }
#hero .content .search-box .search-area form .form-input input:focus + img { transform:rotate(90deg); }
#hero .content .search-box .search-area form textarea { float:left; width:100%; height:100px; margin-bottom:10px; }
#hero .content .search-box .search-area form button { font-size:24px; }
#hero .content .search-box .search-area form .chosen-container { float:left; width:100%; margin-bottom:10px; }

