/* ================================================================== CONFIG CAROUSEL */
#myCar{
	width: 100%;
	margin: 15px auto;
	box-shadow: 0 0 15px;
}

#myCar span{ font-size: 50px; }

.carousel-control{ 
	color: #00bfc6 !important;
	transition: all 0.5s; 
}
.carousel-control:hover{ text-shadow: 0 0 15px #00bfc6; }

.carousel-indicators li {
	width: 20px;
	height: 6px;
	margin: 1px;
	border-color: #00bfc6;
	border-radius: 0;
	transition: all 0.5s;
}

.carousel-indicators .active {
	width: 20px;
	height: 6px;
	margin: 1px;
	border-radius: 0;
	background-color: #00bfc6;
	box-shadow: 0 0 15px #00bfc6;
}

.carousel-indicators {
	bottom: 0 !important;
	left: initial;
	right: 1px;
	width: auto;
	padding-left: 0;
	margin-left: 0;
}

